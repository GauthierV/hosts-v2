    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ host_table.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ host_table.name }}</td>
            </tr>
            <tr>
                <th>Adress</th>
                <td>{{ host_table.adress }}</td>
            </tr>
            <tr>
                <th>Address2</th>
